{
  "mode": "yolo",
  "depth": "standard",
  "parallelization": true,
  "commit_docs": true,
  "pr_workflow": true,
  "model_profile": "quality",
  "workflow": {
    "research": true,
    "plan_check": true,
    "verifier": true
  },
  "github": {
    "enabled": true,
    "issueMode": "auto"
  },
  "workflows": {
    "execute-phase": {
      "post_task_command": "bun run typecheck:all"
    },
    "verify-work": {
      "extra_verification_commands": [
        "bun run typecheck:all",
        "bun run lint:electron"
      ]
    }
  }
}